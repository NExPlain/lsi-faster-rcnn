layer {
  bottom: "cls_score"
  bottom: "labels"
  top: "loss_cls"
  name: "infogainloss_cls"
  type: "InfogainLoss"
  propagate_down: 1
  propagate_down: 0
  loss_weight: 2
  infogain_loss_param {
    source: "./models/kitti/MobileNet/frcnn+or_3cls/infogainH.binaryproto"
  }
  loss_param {
    ignore_label: -1
    normalization: VALID
  }
}

layer {
  name: "loss_bbox"
  type: "SmoothL1Loss"
  bottom: "bbox_pred"
  bottom: "bbox_targets"
  bottom: "bbox_inside_weights"
  bottom: "bbox_outside_weights"
  top: "loss_bbox"
  loss_weight: 1
}
layer {
  name: "loss_viewp"
  type: "SoftmaxWithLoss"
  bottom: "viewpoints_crop"
  bottom: "gt_viewpoints_pd"
  propagate_down: 1
  propagate_down: 0
  top: "viewp_loss"
  loss_weight: 1
  loss_param {
    ignore_label: -10
    normalize: true
  }
}
