layer {
  name: "cls_score-mn"
  type: "InnerProduct"
  bottom: "fc7"
  top: "cls_score"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  inner_product_param {
    # N_CLASSES (original: 21)
    num_output: 4
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "bbox_pred-mn"
  type: "InnerProduct"
  bottom: "fc7"
  top: "bbox_pred"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  inner_product_param {
    # 4 * N_CLASSES (original: 84)
    num_output: 16
    weight_filler {
      type: "gaussian"
      std: 0.001
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "viewpoints_pd-mn"
  type: "InnerProduct"
  bottom: "fc7"
  top: "viewpoints_pd"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  inner_product_param {
    # VIEWP_BINS * N_CLASSES (not original)
    num_output: 32
    weight_filler {
      type: "gaussian"
      std: 0.001 # before: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: 'crop-angles'
  type: 'Python'
  bottom: 'viewpoints_pd'
  bottom: 'viewpoints_weights'
  propagate_down: 1
  propagate_down: 0
  top: 'viewpoints_crop'
  python_param {
    module: 'fast_rcnn.crop_angles'
    layer: 'CropAnglesLayer'
  }
}
