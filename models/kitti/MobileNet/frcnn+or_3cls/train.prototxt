name: "MobileNet"
layer {
  name: 'input-data'
  type: 'Python'
  top: 'data'
  top: 'im_info'
  top: 'gt_boxes'
  python_param {
    module: 'roi_data_layer.layer'
    layer: 'RoIDataLayer'
    # N_CLASSES (original: 21)
    param_str: "'num_classes': 4"
  }
}

